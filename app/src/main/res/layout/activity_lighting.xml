<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.eronmac.shome.Lighting">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Lighting"
        android:textAlignment="center"

        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/toggleButton4"
        android:layout_width="150dp"
        android:layout_height="143dp"
        android:autoText="false"
        android:background="@drawable/toggle_onoff"
        android:checked="false"
        android:textOff=""
        android:textOn=""
        android:layout_marginStart="16dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton" />

    <ToggleButton
        android:id="@+id/toggleButton5"
        android:layout_width="148dp"
        android:layout_height="144dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:autoText="false"
        android:background="@drawable/toggle_onoff"
        android:checked="false"
        android:textOff=""
        android:textOn=""
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton6" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="150dp"
        android:layout_height="142dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="177dp"
        android:autoText="false"
        android:background="@drawable/toggle_onoff"
        android:checked="false"
        android:textOff=""
        android:textOn=""
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ToggleButton
        android:id="@+id/toggleButton6"
        android:layout_width="143dp"
        android:layout_height="143dp"
        android:layout_marginStart="16dp"
        android:autoText="false"
        android:background="@drawable/toggle_onoff"
        android:checked="false"
        android:textOff=""
        android:textOn=""
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="177dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <SeekBar
        android:id="@+id/seekBar5"
        android:layout_width="142dp"
        android:layout_height="18dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/toggleButton4"
        android:layout_marginRight="16dp"
        app:layout_constraintVertical_bias="0.526"
        app:layout_constraintRight_toRightOf="parent" />

    <SeekBar
        android:id="@+id/seekBar6"
        android:layout_width="142dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton4"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <SeekBar
        android:id="@+id/seekBar8"
        android:layout_width="142dp"
        android:layout_height="18dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton5"
        android:layout_marginLeft="16dp"
        app:layout_constraintVertical_bias="0.529"
        app:layout_constraintLeft_toLeftOf="parent" />

    <SeekBar
        android:id="@+id/seekBar7"
        android:layout_width="142dp"
        android:layout_height="18dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/toggleButton5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toggleButton6" />

</android.support.constraint.ConstraintLayout>
