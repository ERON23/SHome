<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.eronmac.shome.TasksT1">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="16dp"
        android:text="Lock Home"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="138dp"
        android:layout_height="43dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:entries="@array/Added_Devices"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188" />

    <Button
        android:id="@+id/button"
        android:layout_width="207dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Add Device"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.971"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188"
        tools:ignore="HardcodedText" />

    <TableLayout
        android:layout_width="349dp"
        android:layout_height="404dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="100dp">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="114dp"
                android:layout_height="48dp"
                android:fontFamily="sans-serif"
                android:text="Action"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Display1"
                android:textSize="26sp"
                android:textStyle="bold"
                android:typeface="normal"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="155dp"
                tools:ignore="HardcodedText,UnusedAttribute" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="114dp"
                android:layout_height="48dp"
                android:fontFamily="sans-serif"
                android:text="Device"
                android:textAlignment="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Display1"
                android:textSize="26sp"
                android:textStyle="bold"
                android:typeface="normal"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="155dp"
                tools:ignore="HardcodedText,UnusedAttribute" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Spinner
                android:id="@+id/spinner5"
                android:entries="@array/Actions"
                android:layout_width="116dp"
                android:layout_height="match_parent"
                tools:layout_editor_absoluteX="39dp"
                tools:layout_editor_absoluteY="242dp" />

            <EditText
                android:id="@+id/editText4"
                android:layout_width="232dp"
                android:layout_height="match_parent"
                android:ems="10"
                android:inputType="none"
                android:text="Front Door"
                android:textAlignment="center"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="315dp"
                tools:ignore="HardcodedText" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <Spinner
                android:id="@+id/spinner6"
                android:layout_width="116dp"
                android:layout_height="match_parent"
                android:entries="@array/Actions"
                tools:layout_editor_absoluteX="39dp"
                tools:layout_editor_absoluteY="242dp" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="232dp"
                android:layout_height="match_parent"
                android:ems="10"
                android:inputType="none"
                android:text="Back Door"
                android:textAlignment="center"
                tools:ignore="HardcodedText"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="315dp" />

        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="129dp">

            <Spinner
                android:id="@+id/spinner7"
                android:layout_width="116dp"
                android:layout_height="match_parent"
                android:entries="@array/Actions"
                tools:layout_editor_absoluteX="39dp"
                tools:layout_editor_absoluteY="242dp" />

            <EditText
                android:id="@+id/editText6"
                android:layout_width="232dp"
                android:layout_height="match_parent"
                android:ems="10"
                android:inputType="none"
                android:text="Garage Door"
                android:textAlignment="center"
                tools:ignore="HardcodedText"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="315dp" />

        </TableRow>
    </TableLayout>


</android.support.constraint.ConstraintLayout>
